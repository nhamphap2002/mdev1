<?xml version="1.0"?>
<config>
    <modules>
        <TV_Customcms>
            <version>0.1.0</version>
        </TV_Customcms>
    </modules>
    <global>
        <models>
            <atwixcms>
                <class>TV_Customcms_Model</class>
            </atwixcms>
        </models>
        <events>
            <adminhtml_cms_page_edit_tab_content_prepare_form>
                <observers>
                    <atwix_page_edit_tab_content>
                        <type>singleton</type>
                        <class>TV_Customcms_Model_Observer</class>
                        <method>cmsField</method>
                    </atwix_page_edit_tab_content>
                </observers>
            </adminhtml_cms_page_edit_tab_content_prepare_form>
        </events>
    </global>
</config> 